				  <label value="Tipo Sanción: " id="lblTipoSancion"/>
			      <textbox id="txtTipoSancion" mold="rounded"  cols="5" maxlength="22" value="RP" disabled="true"></textbox>
			      
			      		      
			      
			      <label value="Período Inicial: " id="lblPeriodoInicial"/>
			      <textbox id="txtPeriodoInicial" mold="rounded" cols="8" maxlength="30" value="2013-I" disabled="true"></textbox>
			      
			      <label value="Período Final: " id="lblPeriodoFinal"/>
			      <textbox id="txtPeriodoFinal" mold="rounded" cols="8" maxlength="30" value="2014-I" disabled="true"></textbox>
			     
			     
			      
				</row>
				<row>
				
