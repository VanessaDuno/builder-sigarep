			      <label value="Período Inicial: " id="lblPeriodoInicial1"/>
			      <textbox id="txtPeriodoInicial1" mold="rounded" cols="8" maxlength="30" value="2013-I" disabled="true"></textbox>
			      
			      <label value="Período Final: " id="lblPeriodoFinal1"/>
			      <textbox id="txtPeriodoFina1l" mold="rounded" cols="8" maxlength="30" value="2014-I" disabled="true"></textbox>
			     
			     <label value="Materia: " id="lblMateria"/>
