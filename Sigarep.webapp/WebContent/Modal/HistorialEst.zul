		<separator />
		<separator />
	<div align="center" height="100%">
	   <hbox> 
	    <vbox>
	    
		<button  label="Registro Apelación" mold="trendy" id="btn1" image="/imagenes/portal-principal/fondoAzul.png"  width="150px" height="10px"
