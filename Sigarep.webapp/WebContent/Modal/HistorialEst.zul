<window title="Historial"  id="winHistorial" border="normal"
	width="1200px" height="550px"
	apply=""
	position="center,center" closable="true" 
	action="show: slideDown;hide: slideUp">
	<groupbox>
		<caption label="Datos Estudiante" />
		<grid>
			<columns>
				<column  />
				<column  />
				<column  />
				<column  />
			</columns>
			<rows>
				<row>
					<label value="Cédula: " id="lblCedula"  />
					<textbox id="txtCedula" mold="rounded" cols="23" maxlength="10" value="18571956" disabled="true"></textbox>
					<label value="Nombre: " id="lblNombre"/>
					<textbox id="txtNombre" mold="rounded" cols="23" maxlength="30" value="María" disabled="true"></textbox>
				</row>
				<row>
					<label value="Apellido: " id="lblApellido" />
					<textbox id="txtApellido" mold="rounded" cols="23" maxlength="30"
						value="Pérez" disabled="true">
					</textbox>
					<label value="Programa: " id="lblPrograma"/>
					<textbox id="txtPrograma" mold="rounded" cols="23" maxlength="30" value="Ingeniería en Informática"
						disabled="true">
					</textbox>
				</row>
				
		    </rows>
		</grid>		
	</groupbox>
	<groupbox>
